<div class="card-container">

    <div class="artist-card">
        <div class="artist-image">
            <img src={{this.images.[0].url}} alt="">
        </div>
        <div class="card-text">
            <p>{{this.name}}</p>
            <audio controls src={{this.preview_url}} type="audio/mpeg"></audio>
        </div>

    </div>
</div>